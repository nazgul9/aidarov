3D\\u043E\\u0432\\u044F\\u0442\\u0441\\u044F \\u043D\\u0430\\u0448\\u0438\\u043C\\u0438 \\u0434\\u0440\\u0443\\u0437\\u044C\\u044F\\u043C\\u0438 \\u0438 \\u043F\\u0440\\u0438\\u0432\\u043E\\u0434\\u044F\\u0442 \\u043A \\u043D\\u0430\\u043C \\u0447\\u043B\\u0435\\u043D\\u043E\\u0432 \\u0441\\u0435\\u043C\\u044C\\u0438, \\u0430 \\u0442\\u0430\\u043A\\u0436\\u0435 \\u0440\\u0435\\u043A\\u043E\\u043C\\u0435\\u043D\\u0434\\u0443\\u044E\\u0442 \\u043A\\u043B\\u0438\\u043D\\u0438\\u043A\\u0443 \\u0437\\u043D\\u0430\\u043A\\u043E\\u043C\\u044B\\u043C.\"}),/*#__PURE__*/_jsx(Card.Text,{className:\"text_nav mt-5\",children:\"\\u041A\\u0430\\u0436\\u0434\\u0430\\u044F \\u0434\\u0435\\u0442\\u0430\\u043B\\u044C \\u0432 \\u043A\\u043B\\u0438\\u043D\\u0438\\u043A\\u0435, \\u043E\\u0442 \\u043E\\u0431\\u043E\\u0440\\u0443\\u0434\\u043E\\u0432\\u0430\\u043D\\u0438\\u044F \\u0434\\u043E \\u0438\\u043D\\u0442\\u0435\\u0440\\u044C\\u0435\\u0440\\u0430, \\u0441\\u043B\\u0443\\u0436\\u0438\\u0442 \\u0434\\u043B\\u044F \\u0442\\u043E\\u0433\\u043E, \\u0447\\u0442\\u043E\\u0431\\u044B \\u0432\\u044B \\u043F\\u043E\\u0447\\u0443\\u0432\\u0441\\u0442\\u0432\\u043E\\u0432\\u0430\\u043B\\u0438 \\u0441\\u0435\\u0431\\u044F \\u043A\\u043E\\u043C\\u0444\\u043E\\u0440\\u0442\\u043D\\u043E \\u0438 \\u0441\\u0432\\u043E\\u0431\\u043E\\u0434\\u043D\\u043E. \\u041C\\u044B \\u0441\\u043E\\u0437\\u0434\\u0430\\u043B\\u0438 \\u0440\\u0430\\u0441\\u0441\\u043B\\u0430\\u0431\\u043B\\u044F\\u044E\\u0449\\u0443\\u044E \\u0430\\u0442\\u043C\\u043E\\u0441\\u0444\\u0435\\u0440\\u0443, \\u043A\\u043E\\u0442\\u043E\\u0440\\u0443\\u044E \\u0432\\u044B \\u043F\\u043E\\u0447\\u0443\\u0432\\u0441\\u0442\\u0432\\u0443\\u0435\\u0442\\u0435 \\u0443\\u0436\\u0435 \\u0432 \\u0437\\u043E\\u043D\\u0435 \\u0440\\u0435\\u0441\\u0435\\u043F\\u0448\\u0435\\u043D\\u0430.\"})]})]})]})}),/*#__PURE__*/_jsx(\"h1\",{\"data-aos\":\"fade-up\",\"data-aos-anchor-placement\":\"top-bottom\",className:\"text_nav text-center mt-5\",children:\"\\u0417\\u0434\\u0435\\u0441\\u044C \\u0432\\u044B \\u043C\\u043E\\u0436\\u0435\\u0442\\u0435:\"}),/*#__PURE__*/_jsxs(\"div\",{className:\"container row m-3 p-2\",children:[/*#__PURE__*/_jsx(Col,{xs:10,md:6,lg:5,sm:6,children:/*#__PURE__*/_jsx(\"h3\",{\"data-aos\":\"fade-up\",\"data-aos-anchor-placement\":\"top-bottom\",class:\"tn-atom text-center \",children:\"\\u043F\\u043E\\u0447\\u0438\\u0442\\u0430\\u0442\\u044C \\u0436\\u0443\\u0440\\u043D\\u0430\\u043B\\u044B\"})}),/*#__PURE__*/_jsx(Col,{xs:10,md:6,lg:5,children:/*#__PURE__*/_jsx(\"h3\",{\"data-aos\":\"fade-up\",\"data-aos-anchor-placement\":\"top-bottom\",class:\"tn-atom text-center \",children:\"\\u0432\\u044B\\u043F\\u0438\\u0442\\u044C \\u0447\\u0430\\u0448\\u0435\\u0447\\u043A\\u0443 \\u043A\\u043E\\u0444\\u0435\"})}),/*#__PURE__*/_jsx(Col,{xs:10,md:6,lg:5,children:/*#__PURE__*/_jsx(\"h3\",{\"data-aos\":\"fade-up\",\"data-aos-anchor-placement\":\"top-bottom\",class:\"tn-atom text-center \",children:\"\\u043F\\u043E\\u0441\\u043B\\u0443\\u0448\\u0430\\u0442\\u044C \\u043C\\u0443\\u0437\\u044B\\u043A\\u0443\"})}),\" \",/*#__PURE__*/_jsx(Col,{xs:10,md:6,lg:5,children:/*#__PURE__*/_jsx(\"h3\",{\"data-aos\":\"fade-up\",\"data-aos-anchor-placement\":\"top-bottom\",class:\"tn-atom text-center \",children:\"\\u043F\\u043E\\u043E\\u0431\\u0449\\u0430\\u0442\\u044C\\u0441\\u044F \\u0441 \\u0441\\u043E\\u0442\\u0440\\u0443\\u0434\\u043D\\u0438\\u043A\\u0430\\u043C\\u0438\"})}),\" \",/*#__PURE__*/_jsx(Col,{xs:10,md:6,lg:5,children:/*#__PURE__*/_jsx(\"h3\",{\"data-aos\":\"fade-up\",\"data-aos-anchor-placement\":\"top-bottom\",class:\"tn-atom text-center \",children:\"\\u0440\\u0430\\u0441\\u0441\\u043B\\u0430\\u0431\\u0438\\u0442\\u044C\\u0441\\u044F \\u0432 \\u043C\\u0430\\u0441\\u0441\\u0430\\u0436\\u043D\\u043E\\u043C \\u043A\\u0440\\u0435\\u0441\\u043B\\u0435\"})})]}),/*#__PURE__*/_jsx(RunText,{})]});}export default AboutClinicContainer;","map":{"version":3,"names":["React","useEffect","Row","Card","Col","Accordions","AutoVideo","RunText","AOS","imgg","Button1","StuffContainer","DentalNavbar","AboutClinicContainer","setShow","init","refresh","data","title","text","lineHeight"],"sources":["C:/Users/PREDATOR/Desktop/a/aidarov/src/containers/dental/AboutClinicContainer.js"],"sourcesContent":["import React, { useEffect } from 'react';\r\nimport { Row, Card, Col,} from 'react-bootstrap';\r\nimport Accordions from '../../components/accrdion/Accordions';\r\nimport AutoVideo from '../../components/Vidoe/AutoVideo'\r\nimport './dental.scss'\r\nimport RunText from '../../components/runText/RunText';\r\nimport AOS from \"aos\";\r\nimport imgg from '../../img.aidarov/imgg.jpeg'\r\nimport Button1 from '../../Button1';\r\nimport StuffContainer from './StuffContainer';\r\nimport DentalNavbar from './dentalAppbar/DentalNavbar';\r\n\r\n\r\n\r\nfunction AboutClinicContainer({ setShow }) {\r\n    useEffect(() => {\r\n        AOS.init();\r\n        AOS.refresh();\r\n    }, []);\r\n    const data = [\r\n        {\r\n            title: \"Оборудование, которое видит больше, чем врач\",\r\n            text: \"Мы предлагаем нашим пациентам только самые современные в сфере стоматологии технологии и оборудование, которые исключают возможность врачебной ошибки.\"\r\n        },\r\n        {\r\n            title: \"Консилиум врачей по преображению каждой улыбки\",\r\n            text: \"За эстетику и функциональность зубов лично отвечают все специалисты клиники, которые участвуют в создании улыбки.\"\r\n        },\r\n        {\r\n            title: \"Работаем даже с самыми сложными случаями\",\r\n            text: \"Опыт наших врачей, современное оборудование и материалы позволяют создать восхитительную улыбку при любой клинической ситуации.\"\r\n        },\r\n        {\r\n            title: \"Высокое качество услуг и сервиса\",\r\n            text: \"Американские технологии и передовые зарубежные методики, привезённые в Россию. Нашим врачам доверяют улыбки популярные люди и члены их семьей.\"\r\n        },\r\n        {\r\n            title: \"Пошаговый план лечения без скрытых расходов\",\r\n            text: \"Полный чекап полости рта и составление цифрового плана лечения с предоставлением наглядных пошаговых действий и их стоимости.\"\r\n        },\r\n    ]\r\n\r\n    return (\r\n        <div className={'container aboutClinic mt-5'}>\r\n            <div className='mt-5 text-center '>\r\n                <DentalNavbar />\r\n                <h1 className='text-center'>С нами вы<br /> улыбаетесь чаще</h1>\r\n                <Row>\r\n                    <Col sm={6} xs={12} md={6} >\r\n                        <p className='text_nav  h5 mt-5'> Команда клиники «Лаборатория улыбок Dr. Айдаров<br />\r\n                            поможет обрести улыбку мечты. Ответственность. <br />\r\n                            Профессионализм. Забота о каждом пациенте.</p>\r\n                        <Col className='mt-3' sm={6}>\r\n                            <Button1 setShow={setShow} />\r\n                        </Col>\r\n                    </Col>\r\n\r\n                    <Col sm={6} xs={12} md={6}>\r\n                        <AutoVideo />\r\n                    </Col>\r\n                </Row>\r\n\r\n            </div>\r\n\r\n            <div className='text-center ' data-aos=\"fade-up\">\r\n                <h1 className='mt-5'>5 причин доверить нам улыбку</h1>\r\n            </div>\r\n            <div className=''>\r\n                <Accordions name={data} />\r\n            </div>\r\n            <div className='mt-3'>\r\n                <Row>\r\n                    <Col xs={12} md={4} >\r\n                        <img data-aos=\"fade-up-right\" className='w-100 mb-2' src='https://thumb.tildacdn.com/tild6564-6235-4330-b565-353437343734/-/format/webp/IMG_1364_1.jpeg' alt='' />\r\n                    </Col>\r\n                    <Col xs={12} md={4} >\r\n                        <img data-aos=\"fade-down\"\r\n                            data-aos-easing=\"linear\"\r\n                            data-aos-duration=\"1500\" className='w-100 mb-2' src='https://thumb.tildacdn.com/tild3939-3139-4163-b233-373731323231/-/format/webp/IMG_1368.jpeg' alt='' /></Col>\r\n                    <Col xs={12} md={4}>\r\n                        <img data-aos=\"fade-left\" className='w-100 mb-2' src='https://thumb.tildacdn.com/tild3038-6366-4265-a133-616538313461/-/format/webp/IMG_1374.jpeg' alt='' />\r\n                    </Col>\r\n                </Row>\r\n            </div>\r\n            <div className=' mt-5'>\r\n                <Row>\r\n                    <Col xs={12} md={6} lg={6}>\r\n                        <div data-aos=\"zoom-in\" className='justify-content-evenly a text-center' >\r\n                            <img class=\" t013__img t-img\" src={imgg} imgfield=\"img\" data-tu-max-width=\"320\" data-tu-max-height=\"320\" data-tu-cover=\"c\" alt=\"\" role=\"presentation\">\r\n                            </img>\r\n                            <Card.Title className='text_nav'>Доктор Айдаров</Card.Title>\r\n                            <h6 className='text_nav' style={{  lineHeight: '18px' }} data-customstyle=\"yes\">Основатель и<br /> главный врач клиники, <br />стоматолог-ортопед</h6>\r\n                        </div>\r\n                    </Col>\r\n                    <Col xs={12} md={6} lg={6}>\r\n                        <div data-aos=\"zoom-in-up\" className='justify-content-evenly text_nav '>\r\n                            <div >\r\n                                Я хотел, чтобы клиника не напоминала больницу. Поэтому мы создали пространство, которое настраивает<br />\r\n                                на правильную волну совместного творчества. Если люди приходят лечить зубы как на каторгу, то сложно <br />\r\n                                ожидать от них высокогоуровня мотивации. Все мы немного нервничаем, если вокруг больничные<br /> белые стены. А у нас даже зона\r\n                                ресепшена больше похожа на лобби-бар в отеле, где во время ожидания можно <br />послушать музыку, почитать\r\n                                что-нибудь интересное или выпить чашечку кофе.\r\n                            </div>\r\n                            <div className='mt-5'>\r\n                                Все пациенты — наши друзья! Те, кто приходит сюда впервые, знакомятся с нами и, пройдя вместе путь <br />от стоматологического кресла до красивой улыбки и здоровых зубов, остаются с нашей командой в прекрасных отношениях.\r\n                            </div>\r\n                        </div>\r\n                    </Col>\r\n                </Row>\r\n            </div>\r\n\r\n            <div className='mt-5'>\r\n                <Row >\r\n                    <Col xs={12} md={6} lg={6}>\r\n                        <div className='d-block'><span className=' h1'>Давайте познакомимся <br />поближе</span>\r\n                            <div className='text_nav '>\r\n                                Стоматологическая клиника «Лаборатория улыбок»<br />\r\n                                преображает улыбки пациентов с 2015 года. Её основатель <br />\r\n                                Айдаров прошёл путь от работы в государственной<br />\r\n                                поликлинике до открытия собственной клиники в центре<br />\r\n                                Москвы и на своём опыте знает, что главными приоритетами<br />\r\n                                в работе врача являются забота о пациентах и качество услуг.\r\n                            </div>\r\n                        </div>\r\n                    </Col>\r\n                    <Col xs={12} md={6} lg={6}>\r\n                        <img className='w-100' src=\"https://thumb.tildacdn.com/tild3734-6164-4733-a634-663535363130/-/cover/1000x1200/center/center/-/format/webp/IMG_0723.JPG\" />\r\n                        <div className='d-block'>\r\n                            <h2 className=' mt-5 text-white'>Со дня основания в клинике доктора Эльмара созданы сотни красивых улыбок.</h2>\r\n\r\n                            <Card.Text className='text_nav'>\r\n                                Зачастую пациенты становятся нашими друзьями и приводят к нам членов семьи, а также рекомендуют клинику знакомым.\r\n                            </Card.Text>\r\n                            <Card.Text className='text_nav mt-5'>Каждая деталь в клинике, от оборудования до интерьера, служит для того, чтобы вы почувствовали себя комфортно и свободно. Мы создали расслабляющую атмосферу, которую вы почувствуете уже в зоне ресепшена.\r\n                            </Card.Text>\r\n                        </div>\r\n                    </Col>\r\n                </Row>\r\n            </div>\r\n            <h1 data-aos=\"fade-up\" data-aos-anchor-placement=\"top-bottom\" className='text_nav text-center mt-5'>Здесь вы можете:</h1>\r\n            <div className='container row m-3 p-2'>\r\n                <Col xs={10} md={6} lg={5} sm={6}>\r\n                    <h3 data-aos=\"fade-up\"\r\n                        data-aos-anchor-placement=\"top-bottom\" class=\"tn-atom text-center \">почитать журналы</h3>\r\n                </Col>\r\n                <Col xs={10} md={6} lg={5}>\r\n                    <h3 data-aos=\"fade-up\"\r\n                        data-aos-anchor-placement=\"top-bottom\" class=\"tn-atom text-center \">выпить чашечку кофе</h3>\r\n                </Col>\r\n                <Col xs={10} md={6} lg={5}>\r\n                    <h3 data-aos=\"fade-up\"\r\n                        data-aos-anchor-placement=\"top-bottom\" class=\"tn-atom text-center \">послушать музыку</h3>\r\n                </Col> <Col xs={10} md={6} lg={5}>\r\n                    <h3 data-aos=\"fade-up\"\r\n                        data-aos-anchor-placement=\"top-bottom\" class=\"tn-atom text-center \">пообщаться с сотрудниками</h3>\r\n                </Col> <Col xs={10} md={6} lg={5}>\r\n                    <h3 data-aos=\"fade-up\"\r\n                        data-aos-anchor-placement=\"top-bottom\" class=\"tn-atom text-center \">расслабиться в массажном кресле</h3>\r\n                </Col>\r\n            </div>\r\n           \r\n            <RunText />\r\n\r\n        </div>\r\n    );\r\n}\r\n\r\nexport default AboutClinicContainer;\r\n\r\n"],"mappings":"AAAA,MAAOA,MAAP,EAAgBC,SAAhB,KAAiC,OAAjC,CACA,OAASC,GAAT,CAAcC,IAAd,CAAoBC,GAApB,KAA+B,iBAA/B,CACA,MAAOC,WAAP,KAAuB,sCAAvB,CACA,MAAOC,UAAP,KAAsB,kCAAtB,CACA,MAAO,eAAP,CACA,MAAOC,QAAP,KAAoB,kCAApB,CACA,MAAOC,IAAP,KAAgB,KAAhB,CACA,MAAOC,KAAP,KAAiB,6BAAjB,CACA,MAAOC,QAAP,KAAoB,eAApB,CACA,MAAOC,eAAP,KAA2B,kBAA3B,CACA,MAAOC,aAAP,KAAyB,6BAAzB,C,wFAIA,QAASC,qBAAT,MAA2C,IAAXC,QAAW,MAAXA,OAAW,CACvCb,SAAS,CAAC,UAAM,CACZO,GAAG,CAACO,IAAJ,GACAP,GAAG,CAACQ,OAAJ,GACH,CAHQ,CAGN,EAHM,CAAT,CAIA,GAAMC,KAAI,CAAG,CACT,CACIC,KAAK,CAAE,8CADX,CAEIC,IAAI,CAAE,wJAFV,CADS,CAKT,CACID,KAAK,CAAE,gDADX,CAEIC,IAAI,CAAE,mHAFV,CALS,CAST,CACID,KAAK,CAAE,0CADX,CAEIC,IAAI,CAAE,iIAFV,CATS,CAaT,CACID,KAAK,CAAE,kCADX,CAEIC,IAAI,CAAE,gJAFV,CAbS,CAiBT,CACID,KAAK,CAAE,6CADX,CAEIC,IAAI,CAAE,+HAFV,CAjBS,CAAb,CAuBA,mBACI,aAAK,SAAS,CAAE,4BAAhB,wBACI,aAAK,SAAS,CAAC,mBAAf,wBACI,KAAC,YAAD,IADJ,cAEI,YAAI,SAAS,CAAC,aAAd,uEAAqC,aAArC,4FAFJ,cAGI,MAAC,GAAD,yBACI,MAAC,GAAD,EAAK,EAAE,CAAE,CAAT,CAAY,EAAE,CAAE,EAAhB,CAAoB,EAAE,CAAE,CAAxB,wBACI,WAAG,SAAS,CAAC,mBAAb,4QAAiF,aAAjF,wQACmD,aADnD,oOADJ,cAII,KAAC,GAAD,EAAK,SAAS,CAAC,MAAf,CAAsB,EAAE,CAAE,CAA1B,uBACI,KAAC,OAAD,EAAS,OAAO,CAAEL,OAAlB,EADJ,EAJJ,GADJ,cAUI,KAAC,GAAD,EAAK,EAAE,CAAE,CAAT,CAAY,EAAE,CAAE,EAAhB,CAAoB,EAAE,CAAE,CAAxB,uBACI,KAAC,SAAD,IADJ,EAVJ,GAHJ,GADJ,cAqBI,YAAK,SAAS,CAAC,cAAf,CAA8B,WAAS,SAAvC,uBACI,WAAI,SAAS,CAAC,MAAd,6JADJ,EArBJ,cAwBI,YAAK,SAAS,CAAC,EAAf,uBACI,KAAC,UAAD,EAAY,IAAI,CAAEG,IAAlB,EADJ,EAxBJ,cA2BI,YAAK,SAAS,CAAC,MAAf,uBACI,MAAC,GAAD,yBACI,KAAC,GAAD,EAAK,EAAE,CAAE,EAAT,CAAa,EAAE,CAAE,CAAjB,uBACI,YAAK,WAAS,eAAd,CAA8B,SAAS,CAAC,YAAxC,CAAqD,GAAG,CAAC,+FAAzD,CAAyJ,GAAG,CAAC,EAA7J,EADJ,EADJ,cAII,KAAC,GAAD,EAAK,EAAE,CAAE,EAAT,CAAa,EAAE,CAAE,CAAjB,uBACI,YAAK,WAAS,WAAd,